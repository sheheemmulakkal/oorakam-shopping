{{#if status}}
<section>
    <div class="container">
        <table class="table mt-5">
            <thead>
                <tr>
                    <th scope="col">Item</th>
                    <th scope="col">Title</th>
                    <th scope="col">Price</th>
                    <th scope="col">Quantity</th>
                </tr>
            </thead>
            <body>
                {{#each response}}
                <tr>
                    
                    <td><img style="width: 70px;height:70px" src="" alt=""></td>
                    <td></td>
                    <td></td>

                    <td>
                       
                        <button  class="cart-item-count mr-3" onclick="changeproductQuantity(-1)">-</button>
                        
                        <span id="qty">1</span>

                        <button class="cart-item-count ml-3" onclick="changeproductQuantity(1)" >+</button>
                    </td>
                    <td>
                        <a href=""  class="btn btn-danger">Remove</a>
                    </td>
                </tr>
                {{/each}}
            </body>
        </table>

        <hr>
        <div class="float-right pr-5">
            <h3 class="float-left mr-5 ">Total : Rs. <span id="total"></span></h3> <br>
        <a href="" class="mt-3 btn btn-success" style="width: ;100%"> <b>Place order</b> </a>
        </div>
    </div>
</section>
{{else}}
<div class="text-center">
    <br><br><br>
    <h1>Your Cart is Empty</h1> <br><br>
    <a class="btn btn-primary" href="">View Products</a>
</div>

{{/if}}

